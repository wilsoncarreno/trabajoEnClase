Proceso notaDefinitiva
	definir prev1, prev2,not3,exFin, p1,p2,n3,ex, prom como real;
	escribir "ingrese la nota del primer previo"
	leer prev1
	escribir "ingrese la nota del segundo previo"
	leer prev2
	escribir "ingrese la 3 nota "
	leer not3
	escribir "ingrese la nota final del examen final previo"
	leer exFin
	p1=prev1*0.25
	p2=prev2*0.25
	n3=not3*0.20
	ex=exFin*0.30
	prom=p1+p2+n3+ex
	escribir "el promedio es de ", prom;
	
	
	
FinProceso
